{
    "name": "ahmedtofaha/laravel-insights",
    "description": "Laravel Points System Package",
    "keywords": [
        "ahmedtofaha",
        "laravel",
        "laravel-insights",
        "points",
        "points-system"
    ],
    "homepage": "https://github.com/ahmedtofaha10/laravel-insights",
    "license": "MIT",
    "authors": [
        {
            "name": "ahmedtofaha10",
            "email": "ahmedtofaha999@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "AhmedTofaha\\LaravelInsights\\": "src",
            "AhmedTofaha\\LaravelInsights\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmedTofaha\\LaravelInsights\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedTofaha\\LaravelInsights\\LaravelInsightsServiceProvider"
            ],
            "aliases": {
                "AhmedTofaha\\LaravelInsights\\Facades\\LaravelInsights": "LaravelInsights"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
